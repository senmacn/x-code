{
  "name": "x-code",
  "version": "1.0.0",
  "description": "Fetch latest posts from followed users on X via proxy",
  "main": "dist/cli/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/cli/index.js start",
    "dev": "ts-node src/cli/index.ts start",
    "fetch-once": "ts-node src/cli/index.ts fetch-once",
    "show": "ts-node src/cli/index.ts show",
    "serve": "ts-node src/api/server.ts",
    "dev:web": "cd web && npm run dev",
    "dev:all": "concurrently \"npm run serve\" \"npm run dev:web\""
  },
  "keywords": [],
  "author": "senmacn",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^8.4.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "https-proxy-agent": "^7.0.6",
    "node-cron": "^4.2.1",
    "pino": "^8.14.1",
    "twitter-api-v2": "^1.27.0",
    "yargs": "^17.7.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.0",
    "@types/yargs": "^17.0.34",
    "concurrently": "^9.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
